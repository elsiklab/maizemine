<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="maizemine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>
  <sources>
	<source name="maize-dna-v4-fasta" type="fasta" dump="true">
		<property name="fasta.className" value="org.intermine.model.bio.Chromosome"/>
		<property name="fasta.loaderClassName" value="org.intermine.bio.dataconversion.MaizeFastaLoaderTask"/>
		<property name="fasta.dataSourceName" value="GRAMENE"/>
		<property name="fasta.dataSetTitle" value="Zm-B73-REFERENCE-GRAMENE-4.0"/>
		<property name="fasta.taxonId" value="4577"/>
		<property name="fasta.includes" value="Zea_mays.AGPv4.dna.toplevel.fa"/>
		<property name="src.data.dir" value="/db/data/zea_mays/AGPv4/fasta/dna/production"/>
	</source>

        <source name="maize-dna-v3-fasta" type="fasta" dump="true">
                <property name="fasta.className" value="org.intermine.model.bio.Chromosome"/>
		<property name="fasta.loaderClassName" value="org.intermine.bio.dataconversion.MaizeFastaLoaderTask"/>
                <property name="fasta.dataSourceName" value="GRAMENE"/>
                <property name="fasta.dataSetTitle" value="B73 RefGen_v3"/>
                <property name="fasta.taxonId" value="4577"/>
                <property name="fasta.includes" value="Zea_mays.AGPv3.31.dna.toplevel.fa"/>
                <property name="src.data.dir" value="/db/data/zea_mays/AGPv3/fasta/dna/production/"/>
        </source>


	<source name="maize-gene-model-v4-gff" type="maize-gff">
                <property name="gff3.seqClsName" value="Chromosome"/>
                <property name="gff3.taxonId" value="4577"/>
                <property name="gff3.seqDataSourceName" value="GRAMENE"/>
                <property name="gff3.dataSourceName" value="GRAMENE"/>
                <property name="gff3.dataSetTitle" value="Zm-B73-REFERENCE-GRAMENE-4.0"/>
                <property name="src.data.dir" value="/db/data/zea_mays/AGPv4/gff3/production/"/>
        </source>
	
	<source name="maize-gene-model-v3-gff" type="maize-gff">
		<property name="gff3.seqClsName" value="Chromosome"/>
                <property name="gff3.taxonId" value="4577"/>
		<property name="gff3.seqDataSourceName" value="GRAMENE"/>
                <property name="gff3.dataSourceName" value="GRAMENE"/>
		<property name="gff3.dataSetTitle" value="B73 RefGen_v3"/>		
                <property name="src.data.dir" value="/db/data/zea_mays/AGPv3/gff3/production/"/>
        </source>
	
	

	<source name="maize-cds-v4-fasta" type="fasta" dump="true">
      		<property name="fasta.className" value="org.intermine.model.bio.CDS"/>
		<property name="fasta.loaderClassName" value="org.intermine.bio.dataconversion.MaizeCDSFastaLoaderTask"/>
		<property name="fasta.dataSourceName" value="GRAMENE"/>
      		<property name="fasta.dataSetTitle" value="Zm-B73-REFERENCE-GRAMENE-4.0"/>
      		<property name="fasta.taxonId" value="4577"/>
      		<property name="fasta.includes" value="Zea_mays.AGPv4.cds.all.fa"/>
      		<property name="src.data.dir" location="/db/data/zea_mays/AGPv4/fasta/cds/"/>
    	</source>

        <source name="maize-cds-v3-fasta" type="fasta" dump="true">
                <property name="fasta.className" value="org.intermine.model.bio.CDS"/>
                <property name="fasta.loaderClassName" value="org.intermine.bio.dataconversion.MaizeCDSFastaLoaderTask"/>
                <property name="fasta.dataSourceName" value="GRAMENE"/>
                <property name="fasta.dataSetTitle" value="B73 RefGen_v3"/>
                <property name="fasta.taxonId" value="4577"/>
                <property name="fasta.includes" value="Zea_mays.AGPv3.31.cds.all.fa"/>
                <property name="src.data.dir" location="/db/data/zea_mays/AGPv3/fasta/cds/"/>
        </source>

	
	<source name="maize-pep-v4-fasta" type="fasta" dump="true">
                <property name="fasta.className" value="org.intermine.model.bio.Peptide"/>
                <property name="fasta.loaderClassName" value="org.intermine.bio.dataconversion.MaizePeptideFastaLoaderTask"/>
		<property name="fasta.sequenceType" value="protein" />
                <property name="fasta.dataSourceName" value="GRAMENE"/>
                <property name="fasta.dataSetTitle" value="Zm-B73-REFERENCE-GRAMENE-4.0"/>
                <property name="fasta.taxonId" value="4577"/>
                <property name="fasta.includes" value="Zea_mays.AGPv4.pep.all.fa"/>
                <property name="src.data.dir" location="/db/data/zea_mays/AGPv4/fasta/pep/"/>
        </source>

        <source name="maize-pep-v3-fasta" type="fasta" dump="true">
                <property name="fasta.className" value="org.intermine.model.bio.Peptide"/>
                <property name="fasta.loaderClassName" value="org.intermine.bio.dataconversion.MaizePeptideFastaLoaderTask"/>
                <property name="fasta.sequenceType" value="protein" />
                <property name="fasta.dataSourceName" value="GRAMENE"/>
                <property name="fasta.dataSetTitle" value="B73 RefGen_v3"/>
                <property name="fasta.taxonId" value="4577"/>
                <property name="fasta.includes" value="Zea_mays.AGPv3.31.pep.all.fa"/>
                <property name="src.data.dir" location="/db/data/zea_mays/AGPv3/fasta/pep/"/>
        </source>

	
	<source name="maize-uniprot" type="uniprot" >
            <property name="uniprot.organisms" value="4577"/>
            <property name="src.data.dir" location="/db/data/zea_mays/uniprot"/>
            <property name="creatego" value="true"/>
            <property name="allowduplicates" value="false"/>
            <property name="loadfragments" value="false"/>
            <property name="loadtrembl" value="true"/>
        </source>	
	
	<source name="uniprot-keywords" type="uniprot-keywords" dump="true">
            <property name="src.data.dir" location="/db/data/zea_mays/uniprot"/>
            <property name="src.data.dir.includes" value="keywlist.xml"/>
        </source>
	
	<source name="interpro" type="interpro">
            <property name="src.data.dir" location="/db/data/zea_mays/interpro"/>
        </source>
	
	<source name="protein2ipr" type="protein2ipr">
    		<property name="src.data.dir" location="/db/data/zea_mays/interpro_protein2ipr"/>
    		<property name="src.data.dir.includes" value="protein2ipr.dat"/>
    		<property name="protein2ipr.organisms" value="4577"/>
	</source>
        <source name="ensemblplant-orthologue" type="ensemblplant">
                 <property name="src.data.dir" location="/db/data/zea_mays/ensemblplant_orthologue"/>
                 <property name="ensemblplant.organisms" value="4577 39946 3702 65489 4533 4538 40148 4528 40149 4536 4537 4529 4530 4555 4565 4572 37682 15368 4513 4558"/>
        </source>
        

        <source name="reactomegramene-pathway" type="reactomegramene">
                 <property name="src.data.dir" location="/db/data/zea_mays/reactome_pathways_02222017"/>
                 <property name="reactomegramene.organisms" value="4577 3702"/>
        </source>

	<source name="go" type="go">
            <property name="src.data.file" location="/db/data/go/go.obo"/>
        </source>
        
        <source name="symbol" type="symbols">
            <property name="src.data.dir" location="/db/data/zea_mays/symbols-descriptionv3v4/"/>
            <property name="taxonId" value="4577"/>
        </source>


	<source name="go-annotation" type="go-annotation">
            <property name="src.data.dir" location="/db/data/zea_mays/gramene/gene_ontology_annotation/"/>
        </source>

	<source name="entrez-organism" type="entrez-organism" dump="true">
		<property name="src.data.file" location="build/organisms.xml"/>
	</source>

        <source name="update-publications" type="update-publications" dump="true">
                 <property name="src.data.file" location="build/publications.xml"/>
                       <property name="cacheDirName" value="build/"/> -->
                       <property name="loadFullRecord" value="true"/> -->
        </source>


  </sources>

  <post-processing>
	<post-process name="create-chromosome-locations-and-lengths"/>
	<post-process name="create-references"/>
	<post-process name="transfer-sequences"/>
	<post-process name="create-overlap-view"/>
	<post-process name="create-location-overlap-index"/>
	<post-process name="do-sources"/>
	<post-process name="create-attribute-indexes"/>
	<post-process name="create-search-index"/>
	<post-process name="summarise-objectstore"/>
	<post-process name="create-autocomplete-index"/>
  </post-processing>

</project>

