<aspects>
  <aspect name="Genes">
    <subtitle>Genome annotation</subtitle>
    <intro-text>
      The gene structure and other genome annotation in MaizeMine are provided by
      a variety of curated source databases.  In most cases MaizeMine includes
      all genome features that have chromosomal locations.
    </intro-text>
  </aspect>

  <aspect name="Alias and DBxref">
    <subtitle>Alias and DBxref</subtitle>
    <intro-text>
      Alias and DBxref
    </intro-text>
    <aspect-source name="Alias and DBxref"/>
  </aspect>

  <aspect name="Transcripts">
    <subtitle>Transcripts</subtitle>
    <intro-text>
      Transcripts
    </intro-text>
  </aspect>

  <aspect name="Expression">
    <subtitle>Gene Expression</subtitle>
    <intro-text>
      Expression
    </intro-text>
  </aspect>

  <aspect name="Proteins">
    <subtitle>Protein and proteomics data</subtitle>
    <intro-text>
      Protein annotation (including family and domain annotation) from UniProt.
    </intro-text>
    <aspect-source name="UniProt" url="http://www.uniprot.org"/>
  </aspect>

  <aspect name="Variation">
    <subtitle>Variation</subtitle>
    <intro-text>
      Variation
    </intro-text>
  </aspect>

  <aspect name="Entire Gene Set">
    <subtitle>Entire Gene Set</subtitle>
    <intro-text>
      Entire Gene Set
    </intro-text>
    <aspect-source name="Entire Gene Set"/>
  </aspect>

  <aspect name="Function">
    <subtitle>GO</subtitle>
    <intro-text>
      Gene Ontology
    </intro-text>
    <aspect-source name="GO" url="http://www.geneontology.org/"/>
    <aspect-source name="InterPro" url="http://www.ebi.ac.uk/interpro/"/>
    <aspect-source name="KEGG" url="http://www.genome.jp/kegg/"/>
  </aspect>

  <aspect name="Homology">
    <subtitle>Orthologues and paralogues</subtitle>
    <intro-text>
      Orthologues and Paralogues.
    </intro-text>
    <aspect-source name="EnsemblPlant" url="http://plants.ensembl.org/info/website/ftp/index.html"/>
  </aspect>

  <aspect name="Pangene">
    <subtitle>Pangene</subtitle>
    <intro-text>
      Pangene
    </intro-text>
  </aspect>

  <aspect name="Publications">
    <subtitle>Publications</subtitle>
    <intro-text>
      Publications
    </intro-text>
    <aspect-source name="Publications" url="http://www.ncbi.nlm.nih.gov/pubmed"/>
  </aspect>

  <aspect name="Community">
    <subtitle>Community data sets</subtitle>
    <intro-text>
      Community data sets
    </intro-text>
    <aspect-source name="Community"/>
  </aspect>
</aspects>

